<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/rt"    tools:context=".PagoActivity">    <TextView        android:id="@+id/textverifica"        android:layout_width="0dp"        android:layout_height="94dp"        android:fontFamily="@font/roboto_condensed_bold"        android:text="Verifique si su número \n está registrado en PAYPHONE."        android:textAlignment="center"        android:textSize="24sp"        android:visibility="visible"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.13"        tools:ignore="MissingConstraints" />    <TextView        android:id="@+id/textrealicepago"        android:layout_width="0dp"        android:layout_height="94dp"        android:fontFamily="@font/roboto_condensed_bold"        android:text="Cliente PAYPHONE registrado. \n Realice su pago."        android:textAlignment="center"        android:textSize="24sp"        android:visibility="visible"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.13"        tools:ignore="MissingConstraints" />    <TextView        android:id="@+id/textdescargueapp"        android:layout_width="0dp"        android:layout_height="94dp"        android:fontFamily="@font/roboto_condensed_bold"        android:text="No se encuentra registrado,\n Descargue la App \n y vuelva a intentarlo."        android:textAlignment="center"        android:textSize="24sp"        android:visibility="visible"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.13999999"        tools:ignore="MissingConstraints" />    <TextView        android:id="@+id/textcompraexito"        android:layout_width="0dp"        android:layout_height="94dp"        android:fontFamily="@font/roboto_condensed_bold"        android:text="Su pago ha sido,\n un EXItO!\n de click para finaizar."        android:textAlignment="center"        android:textSize="24sp"        android:visibility="visible"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.13"        tools:ignore="MissingConstraints" />    <Button        android:id="@+id/buttonpayphone"        android:layout_width="161dp"        android:layout_height="49dp"        android:background="@android:color/holo_orange_dark"        android:text="PAGAR"        android:textAlignment="center"        android:textColor="@color/gris"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <Button        android:id="@+id/buttonppverifica"        android:layout_width="161dp"        android:layout_height="49dp"        android:background="@android:color/holo_green_dark"        android:text="VERIFICAR"        android:textAlignment="center"        android:textColor="@color/gris"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <Button        android:id="@+id/buttonppfin"        android:layout_width="161dp"        android:layout_height="49dp"        android:background="@android:color/holo_purple"        android:onClick="finalizarbutton"        android:text="FINALIZAR"        android:textAlignment="center"        android:textColor="@color/gris"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <Button        android:id="@+id/buttonppvertransid"        android:layout_width="161dp"        android:layout_height="49dp"        android:background="@android:color/holo_blue_light"        android:text="CONTINUE"        android:textAlignment="center"        android:textColor="@color/gris"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <Button        android:id="@+id/buttonlink"        android:layout_width="161dp"        android:layout_height="49dp"        android:background="@android:color/holo_orange_light"        android:text="Descargue App"        android:textAlignment="center"        android:textColor="@color/gris"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <EditText        android:id="@+id/edittextNumberpp"        android:layout_width="140dp"        android:layout_height="42dp"        android:layout_marginEnd="104dp"        android:ems="10"        android:inputType="number"        android:textSize="18sp"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.375"        tools:ignore="MissingConstraints" />    <TextView        android:id="@+id/textViewpp"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:digits="0123456789"        android:text="+593"        android:textSize="20sp"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toStartOf="@+id/edittextNumberpp"        app:layout_constraintHorizontal_bias="0.938"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.38"        tools:ignore="MissingConstraints" />    <Button        android:id="@+id/btnTransferencia"        android:layout_width="130dp"        android:layout_height="50dp"        android:layout_alignParentLeft="true"        android:layout_alignParentEnd="true"        android:layout_alignParentRight="true"        android:layout_alignParentBottom="true"        android:background="@color/colorAccent"        android:text="Transferencia"        android:textColor="@color/common_google_signin_btn_text_dark_default"        android:textSize="12sp"        android:visibility="visible"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent"        tools:text="Transferencia" />    <Button        android:id="@+id/btnPayPhone"        android:layout_width="130dp"        android:layout_height="50dp"        android:layout_alignParentTop="true"        android:layout_alignParentBottom="true"        android:layout_centerHorizontal="true"        android:background="@color/colorAccent"        android:text="Tarjeta\nde Débito"        android:textColor="@color/common_google_signin_btn_text_dark_default"        android:textSize="12sp"        android:visibility="visible"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toStartOf="@+id/btnPaypal"        app:layout_constraintStart_toEndOf="@+id/btnTransferencia"        tools:ignore="MissingConstraints" />    <Button        android:id="@+id/btnPaypal"        android:layout_width="130dp"        android:layout_height="50dp"        android:layout_alignParentLeft="true"        android:layout_alignParentEnd="true"        android:layout_alignParentRight="true"        android:layout_alignParentBottom="true"        android:background="@color/colorAccent"        android:text="Tarjeta\nde Crédito"        android:textColor="@color/common_google_signin_btn_text_dark_default"        android:textSize="12sp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent" />    <ImageView        android:id="@+id/btnpacifico"        android:layout_width="0dp"        android:layout_height="32dp"        android:layout_marginStart="60dp"        android:layout_marginTop="16dp"        android:layout_marginEnd="60dp"        android:contentDescription="@string/logo"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/instruct1a"        app:srcCompat="@drawable/bcpacifico" />    <ImageView        android:id="@+id/btnprodubanco"        android:layout_width="0dp"        android:layout_height="32dp"        android:layout_marginStart="60dp"        android:layout_marginTop="16dp"        android:layout_marginEnd="60dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/btnpacifico"        app:srcCompat="@drawable/bcprodubanco" />    <ImageView        android:id="@+id/btnbolivariano"        android:layout_width="0dp"        android:layout_height="32dp"        android:layout_marginStart="60dp"        android:layout_marginTop="16dp"        android:layout_marginEnd="60dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/btnprodubanco"        app:srcCompat="@drawable/bcbolivariano" />    <ImageView        android:id="@+id/btnguayaquil"        android:layout_width="0dp"        android:layout_height="32dp"        android:layout_marginStart="60dp"        android:layout_marginTop="16dp"        android:layout_marginEnd="60dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/btnbolivariano"        app:srcCompat="@drawable/bcguayaquil" />    <ImageView        android:id="@+id/btnpichincha"        android:layout_width="0dp"        android:layout_height="32dp"        android:layout_marginStart="60dp"        android:layout_marginTop="16dp"        android:layout_marginEnd="60dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/btnguayaquil"        app:srcCompat="@drawable/bcpichincha" />    <TextView        android:id="@+id/txtpreciototalaa"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:background="#00B4A1"        android:fontFamily="@font/roboto_condensed_bold"        android:text="Precio Total"        android:textAlignment="center"        android:textColor="@color/common_google_signin_btn_text_dark_default"        android:textSize="20sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <TextView        android:id="@+id/nro1"        android:layout_width="0dp"        android:layout_height="110dp"        android:layout_marginStart="32dp"        android:layout_marginEnd="8dp"        android:fontFamily="@font/roboto_condensed_bold"        android:gravity="center|top"        android:text="1"        android:textColor="@color/colorPrimary"        android:textSize="100sp"        app:layout_constraintEnd_toStartOf="@+id/instruct1a"        app:layout_constraintHorizontal_bias="0.714"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/txtpreciototalaa" />    <TextView        android:id="@+id/instruct1a"        android:layout_width="0dp"        android:layout_height="94dp"        android:layout_marginStart="90dp"        android:layout_marginTop="36dp"        android:layout_marginEnd="50dp"        android:fontFamily="@font/roboto_condensed_bold"        android:text="Seleecione el banco de su eleccion,para realizar el deposito o transferencia"        android:textSize="18sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/txtpreciototalaa" />    <WebView        android:id="@+id/webViewa"        android:layout_width="0dp"        android:layout_height="0dp"        android:layout_marginTop="27dp"        android:layout_marginBottom="45dp"        android:nestedScrollingEnabled="true"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="1.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <WebView        android:id="@+id/webViewapp"        android:layout_width="0dp"        android:layout_height="0dp"        android:layout_marginTop="27dp"        android:layout_marginBottom="45dp"        android:nestedScrollingEnabled="true"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="1.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" /></androidx.constraintlayout.widget.ConstraintLayout>