<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="#FFF6FF"    tools:context=".PagoActivity">    <ImageView        android:id="@+id/btnpacifico"        android:layout_width="0dp"        android:layout_height="32dp"        android:layout_marginStart="60dp"        android:layout_marginTop="16dp"        android:layout_marginEnd="60dp"        android:contentDescription="@string/logo"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/instruct1a"        app:srcCompat="@drawable/bcpacifico" />    <WebView        android:id="@+id/webViewa"        android:layout_width="0dp"        android:layout_height="0dp"        android:layout_marginTop="27dp"        android:layout_marginBottom="45dp"        android:visibility="gone"        android:nestedScrollingEnabled="true"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="1.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <Button        android:id="@+id/btnTransferencia"        android:layout_width="0dp"        android:layout_height="66dp"        android:layout_marginEnd="2dp"        android:background="@color/colorAccent"        android:fontFamily="@font/roboto_condensed_bold"        android:text="Transferencia"        android:textColor="@color/common_google_signin_btn_text_dark_default"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toStartOf="@+id/btnPaypal"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/webViewa"        app:layout_constraintVertical_bias="1.0"        tools:text="Transferencia" />    <Button        android:id="@+id/btnPaypal"        android:layout_width="0dp"        android:layout_height="66dp"        android:layout_marginStart="2dp"        android:background="@color/colorAccent"        android:fontFamily="@font/roboto_condensed_bold"        android:text="Tarjeta de credito"        android:textColor="@color/common_google_signin_btn_text_dark_default"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toEndOf="@+id/btnTransferencia"        app:layout_constraintTop_toBottomOf="@+id/webViewa"        app:layout_constraintVertical_bias="1.0"        />    <ImageView        android:id="@+id/btnprodubanco"        android:layout_width="0dp"        android:layout_height="32dp"        android:layout_marginStart="60dp"        android:layout_marginTop="16dp"        android:layout_marginEnd="60dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/btnpacifico"        app:srcCompat="@drawable/bcprodubanco" />    <ImageView        android:id="@+id/btnbolivariano"        android:layout_width="0dp"        android:layout_height="32dp"        android:layout_marginStart="60dp"        android:layout_marginTop="16dp"        android:layout_marginEnd="60dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/btnprodubanco"        app:srcCompat="@drawable/bcbolivariano" />    <ImageView        android:id="@+id/btnguayaquil"        android:layout_width="0dp"        android:layout_height="32dp"        android:layout_marginStart="60dp"        android:layout_marginTop="16dp"        android:layout_marginEnd="60dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/btnbolivariano"        app:srcCompat="@drawable/bcguayaquil" />    <ImageView        android:id="@+id/btnpichincha"        android:layout_width="0dp"        android:layout_height="32dp"        android:layout_marginStart="60dp"        android:layout_marginTop="16dp"        android:layout_marginEnd="60dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/btnguayaquil"        app:srcCompat="@drawable/bcpichincha" />    <TextView        android:id="@+id/txtpreciototalaa"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:background="#00B4A1"        android:fontFamily="@font/roboto_condensed_bold"        android:text="Precio Total"        android:textAlignment="center"        android:textColor="@color/common_google_signin_btn_text_dark_default"        android:textSize="20sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <TextView        android:id="@+id/nro1"        android:layout_width="0dp"        android:layout_height="110dp"        android:gravity="center|top"        android:layout_marginStart="32dp"        android:layout_marginEnd="8dp"        android:fontFamily="@font/roboto_condensed_bold"        android:text="1"        android:textColor="@color/colorPrimary"        android:textSize="100sp"        app:layout_constraintEnd_toStartOf="@+id/instruct1a"        app:layout_constraintHorizontal_bias="0.714"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/txtpreciototalaa" />    <TextView        android:id="@+id/instruct1a"        android:layout_width="0dp"        android:layout_height="94dp"        android:layout_marginStart="90dp"        android:layout_marginTop="36dp"        android:layout_marginEnd="50dp"        android:fontFamily="@font/roboto_condensed_bold"        android:text="Seleecione el banco de su eleccion,para realizar el deposito o transferencia"        android:textSize="18sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/txtpreciototalaa" /></androidx.constraintlayout.widget.ConstraintLayout>